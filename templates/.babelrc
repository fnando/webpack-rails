{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    ["@babel/plugin-proposal-function-bind", {"loose": true}]
  ],
  "env": {
    "test": {
      "plugins": ["require-context-hook"]
    }
  }
}
